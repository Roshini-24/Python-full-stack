    "#sum of digits is the divisor of the number\n",
    "n=int(input())\n",
    "n1=n\n",
    "s=0\n",
    "while n>0:\n",
    "    di=n%10\n",
    "    s=s+di\n",
    "    n=n//10\n",
    "print(s)\n",
    "if (n1%s == 0):\n",
    "    print(\"Harshad\")\n",
    "else:\n",
    "    print(\"Not harshad\")\n"
   ]
